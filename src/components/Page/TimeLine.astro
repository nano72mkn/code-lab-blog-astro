---
import ActivityChronology from "@components/ActivityChronology.astro";
import type { ActivityChronology as ActivityChronologyType } from "@utils/groupByYear";

interface Props {
  activityChronologies: ActivityChronologyType[];
}

const { activityChronologies } = Astro.props;

---

{activityChronologies.map((activityChronology) => (
    <ActivityChronology activityChronology={activityChronology} />
))}
